		{
			"name": "auth",
			"item": [
				{
					"name": "Auth ADM e retorna um token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"let jsonData = pm.response.json();",
									"let token = jsonData.accessToken;",
									"let refreshToken = jsonData.refreshToken;",
									"pm.environment.set('accessToken', token);",
									"pm.environment.set('refreshToken', refreshToken);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"{{username}}\",\n  \"password\": \"{{password}}\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseUrl}}/auth/sign-in",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"auth",
								"sign-in"
							]
						}
					},
					"response": []
				},
				{
					"name": "Refresh token Auth ADM and return token",
					"request": {
						"method": "PUT",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{refreshToken}}",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseUrl}}/auth/refresh/{{username}}",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"auth",
								"refresh",
								"{{username}}"
							]
						}
					},
					"response": []
				}
			]
		},
