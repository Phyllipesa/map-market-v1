{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "1e1de97b-a744-405a-8b5a-0397bb3d01ce"
  },
  "newProjectRoot": "projects",
  "projects": {
    "mapMarketFV1": {
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {},
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "assets": [],
            "index": "src/index.html",
            "browser": "src/main.ts",
            "outputPath": "dist/mapMarketFV1",
            "polyfills": ["zone.js"],
            "scripts": [],
            "styles": ["src/global_styles.scss"],
            "tsConfig": "tsconfig.app.json"
          },
          "configurations": {
            "development": {
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true
            },
            "production": {
              "aot": true,
              "extractLicenses": true,
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false
            },
            "local": {
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.prod.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "liveReload": false
          },
          "defaultConfiguration": "development",
          "configurations": {
            "development": {
              "buildTarget": "mapMarketFV1:build:development"
            },
            "production": {
              "buildTarget": "mapMarketFV1:build:production"
            },
            "local": {
              "buildTarget": "mapMarketFV1:build:local"
            }
          }
        }
      }
    }
  },
  "version": 1
}
